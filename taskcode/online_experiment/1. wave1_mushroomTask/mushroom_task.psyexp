      <CodeComponent name="code_7">
        <Param name="Before Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="Before JS Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="Begin Experiment" updates="constant" val="event.clearEvents()&amp;#10;&amp;#10;startGame = 0&amp;#10;&amp;#10;Instr1 = (&quot;BONUS:\n\n&quot;&amp;#10;&quot;At the end of the harvest season you will receive a bonus.\n&quot;&amp;#10;&quot;The amount of this bonus depends on how well you did during the game, and is calculated as follows:\n\n&quot;&amp;#10;&quot;The computer will randomly pick one of the harvest days and compare your decision that day to the correct answer. &quot;&amp;#10;&quot;If your decision was exactly correct, you will receive a bonus of £1.50. For each blue bag off, we will deduct 4 pence.\n\n\n&quot;&amp;#10;&quot;Press the RIGHT arrow key to continue.&quot;)&amp;#10;&amp;#10;Instr2 = (&quot;For example, if you decided to give 70 out of the 100 pickers a blue bag, but there were 88 blue mushrooms in the forest, &quot;&amp;#10;&quot;you will receive £1.50 - 18 x £0.04 = £0.78.\n\n&quot;&amp;#10;&quot;Your bonus will never go below £0.00.\n\n&quot;&amp;#10;&quot;This means that each and every day might determine your bonus at the end of the season.\n\n\n&quot;&amp;#10;&quot;Press the RIGHT arrow key to continue.&quot;)&amp;#10;&amp;#10;Instr3 = &quot;Importanty, you can increase your chances of earning a high bonus by rating your confidence in your estimate:\n\n&quot;&amp;#10;&quot;If you rate your confidence as MEDIUM, the chance that the computer will pick that day will be TWO times as high as when you would rate your confidence as LOW.\n\n&quot;&amp;#10;&quot;If you rate your confidence as HIGH, the chance that the computer will pick that day will be THREE times as high as when you would rate your confidence as LOW.\n\n&quot;&amp;#10;&quot;Thus, if you think that on a certain day your estimate is close to the correct answer, &quot;&amp;#10;&quot;you can increase the chance that the computer will pick that particular day by giving a higher confidence rating.\n\n\n&quot;&amp;#10;&quot;Press the RIGHT arrow key to continue.&quot;)&amp;#10;&amp;#10;Instr4 = (&quot;As said before, the game will consist of multiple harvest days and all days together represent the harvest season. &quot;&amp;#10;&quot;The entire season consists of 3 blocks. After each block, you can take a short break before continuing with the next block.\n\n\n\n&quot;&amp;#10;&quot;If everything is clear, you can press ENTER to start the game&quot;)&amp;#10;&amp;#10;&amp;#10;InstrDict = { &quot;1&quot;: Instr1,&amp;#10;              &quot;2&quot;: Instr2,&amp;#10;              &quot;3&quot;: Instr3,&amp;#10;              &quot;4&quot;: Instr4,&amp;#10;}&amp;#10;&amp;#10;currentInstr = &quot;1&quot;" valType="extendedCode"/>
        <Param name="Begin JS Experiment" updates="constant" val="event.clearEvents();&amp;#10;startGame = 0;&amp;#10;&amp;#10;Instr1 = &quot;BONUS:\n\nAt the end of the harvest season you will receive a bonus.\nThe amount of this bonus depends on how well you did during the game, and is calculated as follows:\n\nThe computer will randomly pick one of the harvest days and compare your decision that day to the correct answer. If your decision was exactly correct, you will receive a bonus of \u00a31.50. For each blue bag off, we will deduct 4 pence.\n\n\nPress the RIGHT arrow key to continue.&quot;&amp;#10;&amp;#10;Instr2 = &quot;For example, if you decided to give 70 out of the 100 pickers a blue bag, but there were 88 blue mushrooms in the forest, you will receive \u00a31.50 - 18 x \u00a30.04 = \u00a30.78.\n\nYour bonus will never go below \u00a30.00.\n\nThis means that each and every day might determine your bonus at the end of the season.\n\n\nPress the RIGHT arrow key to continue.&quot;&amp;#10;&amp;#10;Instr3 = &quot;Importanty, you can increase your chances of earning a high bonus by rating your confidence in your estimate:\n\nIf you rate your confidence as MEDIUM, the chance that the computer will pick that day will be TWO times as high as when you would rate your confidence as LOW.\n\nIf you rate your confidence as HIGH, the chance that the computer will pick that day will be THREE times as high as when you would rate your confidence as LOW.\n\nThus, if you think that on a certain day your estimate is close to the correct answer, you can increase the chance that the computer will pick that particular day by giving a higher confidence rating.\n\n\nPress the RIGHT arrow key to continue.&quot;&amp;#10;&amp;#10;Instr4 = &quot;As said before, the game will consist of multiple harvest days and all days together represent the harvest season. The entire season consists of 3 blocks. After each block, you can take a short break before continuing with the next block.\n\n\n\nIf everything is clear, you can press ENTER to start the game.&quot;&amp;#10;&amp;#10;InstrDict = {&quot;1&quot;: Instr1, &quot;2&quot;: Instr2, &quot;3&quot;: Instr3, &quot;4&quot;: Instr4};&amp;#10;currentInstr = &quot;1&quot;;&amp;#10;" valType="extendedCode"/>
        <Param name="Begin JS Routine" updates="constant" val="progress_bar_size = (((window_x * 2) / 13) * (Number.parseInt(currentInstr) + 9));&amp;#10;progress_bar_pos = ((- window_x) + ((((window_x * 2) / 13) * (Number.parseInt(currentInstr) + 9)) / 2));&amp;#10;progress_text = ((&quot;Instructions: &quot; + (Number.parseInt(currentInstr)+9).toString()) + &quot;/13&quot;);&amp;#10;" valType="extendedCode"/>
        <Param name="Begin Routine" updates="constant" val="progress_bar_size = (window_x*2)/13 * (int(currentInstr) + 9)&amp;#10;progress_bar_pos = (-window_x) + (window_x*2)/13 * (int(currentInstr) + 9) / 2&amp;#10;progress_text = &quot;Instructions: &quot; + str(int(currentInstr)+9) + &quot;/13&quot;&amp;#10;" valType="extendedCode"/>
        <Param name="Code Type" updates="None" val="Both" valType="str"/>
        <Param name="Each Frame" updates="constant" val="" valType="extendedCode"/>
