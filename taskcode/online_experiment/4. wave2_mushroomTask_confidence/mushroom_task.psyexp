        <Param name="End JS Routine" updates="constant" val="window_dimension = (win.size[0] / win.size[1]);&amp;#10;window_y = 0.5;&amp;#10;window_x = (window_y * window_dimension);&amp;#10;aperture_size = (window_y * 0.4);&amp;#10;mushroom_size = (aperture_size * 0.5);&amp;#10;" valType="extendedCode"/>
